{
  "name": "raydium-orca-arbitrage",
  "version": "2.0.0",
  "description": "Cross-DEX arbitrage bot supporting Raydium and Orca",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "node verify-setup.js",
    "scanner": "ts-node GrpcFastScanner.ts",
    "scanner:hft": "ts-node UltraFastGrpcScanner.ts",
    "executor": "ts-node ArbitrageBot.ts",
    "executor:once": "ts-node ArbitrageBot.ts --once",
    "executor:fast": "ts-node FastExecutor.ts",
    "hft": "ts-node SimpleHFTBot.ts",
    "bot:hft": "ts-node HFTArbitrageBot.ts",
    "bot:simple": "ts-node SimpleHFTBot.ts",
    "bot:multipool": "ts-node RealMultiPoolHFTBot.ts",
    "bot:multipool:live": "cross-env DRY_RUN=false ts-node RealMultiPoolHFTBot.ts",
    "bot:optimized": "ts-node OptimizedHFTBot.ts",
    "bot:optimized:live": "cross-env DRY_RUN=false ts-node OptimizedHFTBot.ts",
    "allhops": "ts-node AllHopsScanner.ts",
    "allhops:live": "cross-env DRY_RUN=false ts-node AllHopsScanner.ts",
    "start": "concurrently \"npm run scanner\" \"npm run executor:fast\"",
    "simulator:4hop": "ts-node FourHopSimulator.ts",
    "simulator:4hop:test": "ts-node test-4hop.ts",
    "bot:multipath": "ts-node MultiPathArbitrageBot.ts",
    "view-logs": "node view-logs.js",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIMaxLabz/Sky_O2O.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/AIMaxLabz/Sky_O2O/issues"
  },
  "homepage": "https://github.com/AIMaxLabz/Sky_O2O#readme",
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@mercurial-finance/dynamic-amm-sdk": "^1.1.23",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@meteora-ag/dlmm": "^1.9.3",
    "@orca-so/common-sdk": "^0.7.0",
    "@orca-so/whirlpools": "^6.0.0",
    "@orca-so/whirlpools-sdk": "^0.17.4",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@raydium-io/raydium-sdk-v2": "^0.2.32-alpha",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@triton-one/yellowstone-grpc": "^5.0.1",
    "@types/bn.js": "^5.2.0",
    "axios": "^1.13.2",
    "bn.js": "^5.2.2",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "jito-ts": "^4.2.1",
    "p-retry": "^7.1.1",
    "phoenix-sdk": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
